{
  "name": "adon-api",
  "version": "2.1.0",
  "description": "API for all Adon-based projects",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "scripts": {
    "start:server": "nodemon --config nodemon.json --exec ts-node sample/index.ts",
    "audit": "npm audit",
    "lint": "eslint \"src/**/*.ts\"",
    "snyk": "snyk test",
    "test": "jest --logHeapUsage",
    "test:coverage": "jest--logHeapUsage --coverage",
    "coveralls": "jest --logHeapUsage --coverage && cat ./coverage/lcov.info | coveralls",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisv79/ADON-API.git"
  },
  "keywords": [
    "adon",
    "api"
  ],
  "author": "Adonis Lee Villamor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adonisv79/ADON-API/issues"
  },
  "homepage": "https://github.com/adonisv79/ADON-API#readme",
  "dependencies": {
    "@godaddy/terminus": "4.7.1",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.3",
    "helmet": "4.4.1",
    "http-error-types": "1.0.1",
    "ioredis": "4.26.0",
    "morgan": "1.10.0",
    "rate-limiter-flexible": "2.2.1",
    "rc": "^1.2.8",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/helmet": "4.0.0",
    "@types/ioredis": "4.22.3",
    "@types/jest": "26.0.22",
    "@types/morgan": "1.9.2",
    "@types/node": "14.14.37",
    "@types/rc": "1.1.0",
    "@types/supertest": "^2.0.11",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "eslint": "7.24.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "snyk": "1.522.0",
    "supertest": "6.1.3",
    "ts-jest": "26.5.4",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  }
